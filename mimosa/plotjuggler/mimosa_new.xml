 <customMathEquations>
  <snippet name="imu_rate_odom_rot_diff_deg">
   <global></global>
   <function>x1 = value
y1 = v1
z1 = v2
w1 = v3
x2 = v4
y2 = v5
z2 = v6
w2 = v7

-- Normalize the first quaternion
local norm1 = math.sqrt(w1*w1 + x1*x1 + y1*y1 + z1*z1)
local qw1 = w1 / norm1
local qx1 = x1 / norm1
local qy1 = y1 / norm1
local qz1 = z1 / norm1

-- Normalize the second quaternion
local norm2 = math.sqrt(w2*w2 + x2*x2 + y2*y2 + z2*z2)
local qw2 = w2 / norm2
local qx2 = x2 / norm2
local qy2 = y2 / norm2
local qz2 = z2 / norm2

-- Compute the dot product between the two unit quaternions
local dot = qw1*qw2 + qx1*qx2 + qy1*qy2 + qz1*qz2
-- Clamp dot to the range [-1, 1] to avoid numerical errors
dot = math.max(-1, math.min(1, dot))

-- Calculate the angle (in radians) between the two rotations
local angle = 2 * math.acos(math.abs(dot))

-- Convert to degrees
return angle * 180 / math.pi</function>
